<!DOCTYPE html>
<html>
<head>
  <title>A3</title>
  <%= stylesheet_link_tag :all %>
  <link href="stylesheets/css/pepper-grinder/jquery-ui-1.8.11.custom.css" type="text/css" rel="stylesheet"/>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
  <link rel="stylesheet" type="text/css" href="/stylesheets/pepper-grinder/jquery-ui-1.8.11.custom.css"/>
  <%= yield :head %>
  <script language="javascript" type="text/javascript">
    $(function(){
      $("a.actionbutton").button();
      $('a.actionbutton, .body td a').hover(
					function() { $(this).addClass('ui-state-hover'); },
					function() { $(this).removeClass('ui-state-hover'); });


   $("#accordion").accordion({ header: "h3" });
  });
   

  </script>
  <style>
    a span.ui-icon {float: left; margin: 0 4px;}
    </style>
</head>
<body>

  <div class="menu">
    <%= yield :menu  %>
<ul>
      <li><%=link_to 'Главная',:root%></li>
     
      <li>|<%= render :partial =>"users/user_bar" %></li>
</ul>

  
    
  </div>

   <div id="page">
 

      <table id="top-pads" cellspacing="0" cellpadding="0">
        <tr>
          <%=  yield :tabs%>
        </tr>
      </table>
 <div id ="left-pad">

    <div class="body">
    <%= yield %>
    </div></div>

  </div>

</body>
</html>
