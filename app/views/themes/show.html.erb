<h1><%= @theme.title %></h1>
<% @qtypes=Qtype.all %>
<% @c=6 %>
<% @theme.questions.each do |t| %>
  
  <%= link_to t.content, discipline_theme_question_path(@discipline,@theme,t)%>
  <%=  @qtypes[t.qtype_id-@c].title %><br/>
<% end %>

<%= link_to 'Создать вопрос', new_discipline_theme_question_path(@discipline,@theme) %>

<%= link_to 'Назад', discipline_path(@discipline) %>

