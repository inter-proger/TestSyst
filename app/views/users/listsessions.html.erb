<%= content_for :head do %>

<style>
	td a {margin: 2px; position: relative; padding: 4px 0; cursor: pointer; float: left;  list-style: none;display: inline;}

</style>
<%end%>
<table cellspacing="0" cellpadding="0">
  <tr>
  <th>#</th>
  <th>Дата начала</th>
  <th>Конфигурация</th>
  <th>Оценка</th>
  <th>Завершена</th>
  <th></th>
  </tr>
  <%  @sessions.each do |i|  %>
  <tr>
    <td>#</td>
    <td><%=i.created_at%></td>
    <td><%=i.tconfiguration_id%></td>
    <td>Здесь будет оценка</td>
    <td><% if i.completed %>
      Да
      <%else%>
      Нет
      <%end%>
    </td>
    <td><%=link_to tag("span",{:class=>"ui-icon ui-icon-document"}),{:controller=>:report,:action=>:createdetailreport,:userid=>@us,:id=>i.id},:class=>'ui-state-default ui-corner-all',:title=>'Открыть'%>
    
    </td>
  </tr>
  <%end%>

  </table>

