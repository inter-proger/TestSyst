<h1>Listing configuration_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @configuration_types.each do |configuration_type| %>
  <tr>
    <td><%= configuration_type.Name %></td>
    <td><%= link_to 'Show', configuration_type %></td>
    <td><%= link_to 'Edit', edit_configuration_type_path(configuration_type) %></td>
    <td><%= link_to 'Destroy', configuration_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

